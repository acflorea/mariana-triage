--master=local[1] \
--executor-memory 20G \
--driver-java-options "-Xmx32G -Dconfig.file=./columbugus-conf/netbeans.conf" \
~/phd/columbugus/target/scala-2.10/columbugus-assembly-2.1.jar

~/ibm/spark-dk-1.6.3.0/spark/bin/spark-submit \
--class dr.acf.experiments.ParagraphVector \
--master=local[20] \
